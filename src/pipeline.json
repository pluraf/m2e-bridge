{
"filters": [
  {
    "type": "paththrough",
    "parameters": {
      "fs": 1
    }
  },
  {
    "type": "compare",
    "parameters": {
      "left": "$msg.field_name",
      "right": 10,
      "operation": "lt"
    }
  }
],
"transformers": [
  {
    "type": "match"
  },
  {
    "type": "add",
    "fields": {
      "device_id": "$mqtt_device_id"
    }
  }
],
"mapper": [
  {
    "mqtt_topic": "/topic1",
    "connector": {
      "type": "gcp_pubsub",
      "topic": "/topic2"
    }
  },
  {
    "mqtt_topic": "/#/file",
    "connector": {
      "type": "gcp_pubsub",
      "topic": "$mqtt_msg_topic"
    }
  }
]
}