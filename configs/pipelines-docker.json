{
  "sensor-data":{
    "connector_in": {
      "type": "mqtt",
      "topic": "/topic/+/event",
      "server": "mqtt://localhost:1884",
      "qos": 1,
      "retry_attempts": 5
    },
    "filtras": [
      {
          "type": "comparator",
          "operator": "gt",
          "decoder": "json",
          "value_key": "temp",
          "comparand": 5.4
      },
      {
          "type": "finder",
          "operator": "contain",
          "logical_negation": true,
          "string": "LOG"
      }
  ],
    "connector_out":{
      "type":"gcp_pubsub",
      "key_path":"/etc/m2e-bridge/connectors/auth/feisty-enigma-416319-44f4fddc898c.json",
      "project_id":"feisty-enigma-416319",
      "topic_id":"sensor-data",
      "attributes":{
        "deviceId":"{{topic[2]}}",
        "projectId": "G-NODE"
      }
    }
  },
  "sensor-state":{
    "connector_in": {
      "type": "mqtt",
      "topic": "/topic2",
      "server": "mqtt://localhost:1884",
      "qos": 1,
      "retry_attempts": 5
    },
    "connector_out": {
      "type": "gcp_pubsub",
      "key_path":"/etc/m2e-bridge/connectors/auth/feisty-enigma-416319-44f4fddc898c.json",
      "project_id": "feisty-enigma-416319",
      "topic_id": "sensor-state",
      "attributes":{
        "deviceId":"{{topic[2]}}",
        "projectId": "G-NODE"
      }
    }
  },
  "commands":{
    "connector_in": {
      "type": "gcp_pubsub",
      "key_path":"/etc/m2e-bridge/connectors/auth/feisty-enigma-416319-44f4fddc898c.json",
      "project_id": "feisty-enigma-416319",
      "subscription_id": "commands"
    },
    "connector_out": {
      "type": "mqtt",
      "topic": "/devices/{{node_id}}/commands/{{command}}",
      "server": "mqtt://localhost:1884",
      "qos": 1,
      "retry_attempts": 5
    }
  }
}
